  profile_bookings,\n    profile_tools,\n  };\n\n  const controlProps = {\n    dispatch,\n    menu,\n    logged,\n    username,\n    email,\n    bio,\n    photo,\n    setToken,\n    token,\n  };\n\n  return (\n    <div className=\"App\">\n      <Router>\n        <Header {...controlProps} />\n        <Main className=\"app-main\" {...controlProps}>\n          <Switch>\n            <Route path=\"/login\">\n              <PublicRoute>\n                <CredentialsPage title=\"Log In\" {...controlProps} />\n              </PublicRoute>\n            </Route>\n            <Route path=\"/register\">\n              <PublicRoute>\n                <CredentialsPage title=\"Sign In\" {...controlProps} />\n              </PublicRoute>\n            </Route>\n            <Route path=\"/about\">\n              <AboutPage {...aboutProps} />\n            </Route>\n            <Route path=\"/profile\">\n              <PrivateRoute>\n                <ProfilePage control={controlProps} profile={profileProps} />\n              </PrivateRoute>\n            </Route>\n            <Route path=\"/\">\n              <FlightProvider initialState={initialFlightFormState} reducer={FlightReducer}>\n                <SearchPage {...searchProps} {...controlProps} />\n              </FlightProvider>\n            </Route>\n          </Switch>\n        </Main>\n        <Footer className=\"app-footer\">Code-Vix &copy; 2021 FLanders v0.6</Footer>\n      </Router>\n    </div>\n  );\n};\n\nexport default App;\n","/home/mateo/codigo/booking-flights-app/front/www/src/pages/ProfilePage.jsx",[],"/home/mateo/codigo/booking-flights-app/front/www/src/pages/CredentialsPage.jsx",[],"/home/mateo/codigo/booking-flights-app/front/www/src/components/SearchFlight/SearchForm.jsx",[],"/home/mateo/codigo/booking-flights-app/front/www/src/components/SearchFlight/ResponseFlight.jsx",[],"/home/mateo/codigo/booking-flights-app/front/www/src/components/SearchFlight/ResponseHeader.jsx",[],"/home/mateo/codigo/booking-flights-app/fr